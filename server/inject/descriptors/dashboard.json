{
  "type": "object",
  "properties": {
    "isDashboard": {
      "type": "identifier",
      "selector": "#spanShiftStatus",
      "control": "exists",
      "selectContains": "Total target"
    },
    "totalTarget": {
      "type": "string",
      "selector": "#spanShiftStatus span",
      "control": "property.innerText"
    },
    "totalAct": {
      "type": "string",
      "selector": "#spanShiftStatus span",
      "control": "property.innerText",
      "selectIndex": 1
    },
    "AvrSpeed": {
      "type": "string",
      "selector": "#spanShiftStatus span",
      "control": "property.innerText",
      "selectIndex": 2
    },
    "targetHour": {
      "type": "string",
      "selector": "#spanTarget",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "isDashboard"
      ],
      "screens": [
        "dashboard"
      ]
    }
  ]
}
